    </message>
    <portType name="HotelReservationServicesPortType">
        <operation name="HotelReservationServicesOperation">
            <input name="input1" message="tns:CancelHotelionRequest"/>
            <output name="output1" message="tns:HotelReservationServicesOperationResponse"/>
        </operation>
    </portType>
    <portType name="HotelReservationServicesPortType1">
        <operation name="BookHotel">
            <input name="input1" message="tns:BookHotelRequest"/>
            <output name="output1" message="tns:BookHotelResponse"/>
        </operation>
    </portType>
    <portType name="HotelReservationServicesCancelHotel">
        <operation name="CancelHotel">
            <input name="input1" message="tns:CancelHotelionRequest"/>
        </operation>
    </portType>
    <binding name="HotelReservationServicesPortTypeBinding" type="tns:HotelReservationServicesPortType">
        <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="HotelReservationServicesOperation">
            <soap:operation/>
            <input name="input1">
                <soap:body use="literal"/>
            </input>
